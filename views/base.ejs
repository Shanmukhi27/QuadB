<script>
  function toggleBackground() {
    var body = document.getElementsByTagName("body")[0];
    body.classList.toggle("white");
    var rows = document.getElementsByTagName("tr");
		
			for (var i = 0; i < rows.length; i++) {
				rows[i].classList.toggle("white");
			}
    var dl=document.getElementById("dl");
    dl.classList.toggle("white");
  
  }



  var countdown = 60;
		var timerInterval;

		function startTimer() {
			timerInterval = setInterval(updateTimer, 1000);
		}

		function updateTimer() {
			countdown--;
			if (countdown < 0) {
				countdown = 60;
			}
			var formattedTime = ("00" + countdown).slice(-2);
			document.getElementById("timer").innerHTML = formattedTime;
			var degrees = (1 - (countdown / 60)) * 360;
			document.getElementById("timer").style.backgroundImage = "conic-gradient(#333 " + degrees + "deg, transparent " + degrees + "deg)";
		}
</script>



